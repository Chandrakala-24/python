class card:
        def init(self,ImageURL,Price,Rating,Description,DeliveryWithin,Comments,BrandName):
        self.Image=ImageURL
        self.Cost=Price
        self.Stars=Rating  
        self.information=description
        self.order=deliveryWithin
        self.review=comments
        self.brand=brandName

laptop=card("dummy url1",50000,5,"maxbook","2weeks","this is gud product","apple")
print(laptop.image)
print(laptop.cost)
print(laptop.stars)
print(laptop.information)
print(laptop.order)
print(laptop.review)
print(laptop.brand)


